{
  "name": "planly",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.5",
    "firebase": "^3.2.1",
    "izitoast": "^1.1.1",
    "lodash": "^4.15.0",
    "moment": "^2.14.1",
    "normalize.css": "^7.0.0",
    "react": "^15.6.1",
    "react-datepicker": "^0.52.0",
    "react-dom": "^15.5.4",
    "react-dropdown": "^1.2.5",
    "react-redux": "^5.0.1",
    "react-router-dom": "^4.1.1",
    "react-select": "^1.0.0-rc.5",
    "redux": "^3.5.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.1.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "flow-bin": "^0.52.0",
    "node-sass-chokidar": "0.0.1",
    "npm-run-all": "^4.0.2",
    "react-scripts": "1.0.10",
    "redux-immutable-state-invariant": "^2.0.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "NODE_PATH=src npm-run-all -p watch-css start-js",
    "startsimple": "npm run start-js",
    "build": "NODE_PATH=src npm run build-css && react-scripts build",
    "buildprod": "REACT_APP_TARGET=production npm run build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "flow": "flow"
  }
}
